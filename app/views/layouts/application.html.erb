<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= csrf_meta_tag %>
  <%= render 'layouts/stylesheets' %>
</head>
<body>

<div class="container">
  <%= render 'layouts/header' %>
  <section class="round">
    <% if notice.present? %><p id="notice" class="info"><%= notice %></p><% end %>
    <%= yield %>
  </section>
  <%= render 'layouts/footer' %>
</div>

<%= debug(params) if Rails.env.development? %>

</body>
</html>
